[[do
  [let a 12]
  [let b [+ a 18]]
  [let c [* a b]]

  [let f [fn [a b]
    [let c [+ a b]]
    [let mega 1000000]
    [+ c mega]
  ]]

  [let c [block
    [let a 1000]

    [+ c a]
  ]]
  
  [let - +]
  [let d [- c a]]

  [use a [read_line]]
  [print_line a]
  [use b [read_line]]
  [print_line b]
  [return d]
]]
